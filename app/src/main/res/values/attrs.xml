<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuestionView">
        <attr name="question_text" format="string"/>
        <attr name="question_code" format="string"/>
        <attr name="question_image" format="string"/>
    </declare-styleable>
    <declare-styleable name="AnswerView">
        <attr name="answer_enabled" format="boolean"/>
        <attr name="answer_option" format="string"/>
        <attr name="answer_text" format="string"/>
        <attr name="answer_code" format="string"/>
        <attr name="answer_image" format="string"/>
    </declare-styleable>
    <attr name="background" format="reference"/>
    <attr name="category_background" format="reference"/>
    <attr name="content_background" format="reference"/>
    <attr name="bgColor" format="color"/>
    <attr name="textColor" format="color"/>
    <attr name="metaTextColor" format="color"/>
    <attr name="tab_color" format="reference"/>
</resources>